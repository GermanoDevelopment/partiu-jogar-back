{
  "name": "boilerplate-back",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Germano",
  "license": "MIT",
  "description": "A backend boilerplate application",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nodemon src/main.ts",
    "start:prod": "node dist/main",
    "typeorm": "ts-node node_modules/typeorm/cli.js -d ormconfig",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:create": "npm run typeorm migration:create",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:g": "npm run migration:generate src/database/migrations/%npm_config_name%",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "jest"
  },
  "keywords": [
    "BackendBoilerplate",
    "NestJS",
    "TypeORM",
    "AWS"
  ],
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1295.0",
    "dotenv": "^16.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.0",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/swagger": "^6.0.5",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.7.13",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Germano123/backend-start-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Germano123/backend-start-boilerplate/issues"
  },
  "homepage": "https://github.com/Germano123/backend-start-boilerplate#readme"
}
